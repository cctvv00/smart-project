#include "sys/types.h"
#include "unistd.h"
#include "sys/wait.h"
#include "stdio.h"
#include "stdlib.h"
int main()
    {
        if(fork()==0)
        {
            printf("a \n");            
        }
        else
        {
	    printf("b \n");
            waitpid(-1,NULL,0);
         }
        printf("c \n");
        exit(0);
    return 0;
    }
